server {
        listen 8000;

        # SSL configuration
        #
        # listen 443 ssl default_server;
        # listen [::]:443 ssl default_server;
        #
        # Self signed certs generated by the ssl-cert package
        # Don't use them in a production server!
        #
        # include snippets/snakeoil.conf;

        root /var/www/html/biodatware/biodataware;

        # Add index.php to the list if you are using PHP
        index index.html index.htm index.nginx-debian.html;
        rewrite ^/$ /index.html last;
        server_name _;

         location = /favicon.ico { access_log off; log_not_found off; }
         location /static/ {
                 autoindex on;
                 alias    /var/www/html/biodataware/biodataware/static/;
         }

         location /media/ {
                 autoindex on;
                 alias   /var/www/html/biodataware/biodataware/media/;
         }

         location / {
                 include proxy_params;
                 proxy_pass http://unix:/var/www/html/biodataware/run/gunicorn.sock;
         }

}